import java.util.LinkedHashSet;
import java.util.Set;

public class Test {

	public static void main(String[] args) {
		String s="mmababctamantlslmag";
	   int cnt=0;
	   for(int i=0;i<s.length();i++) {
		   for(int j=i+1;j<s.length();j++) {
			   cnt++;
		   }
		   String d=String.valueOf(s.charAt(i));
		   s=s.replaceAll(d, "");
		   cnt=0;
	   }
	  
	}	
}
